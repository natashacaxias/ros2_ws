  <!--############ CAMERA PAN/TILT  #############-->
  <link name="base_cam">
    <inertial>
      <mass value="1" />
      <origin xyz=" 0 0 0"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
