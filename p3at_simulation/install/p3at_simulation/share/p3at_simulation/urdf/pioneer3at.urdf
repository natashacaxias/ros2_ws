      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
    <visual>
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
      <material name="green"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
    </collision>
  </link>

  <link name="cam_link">
    <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
    <visual>
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
      <material name="ChassisRed">
        <color rgba="0.851 0.0 0.0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
    </collision>
  </link>

  <gazebo reference="base_cam">
    <material>Gazebo/Red</material>
  </gazebo>
  <gazebo reference="cam_link">
    <material>Gazebo/White</material>
  </gazebo>

  <joint name="base_to_pantbl" type="revolute">
    <parent link="base_cam"/>
    <child link="pantbl_link"/>
    <origin xyz="0 0 0.01" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="-1.5" upper="1.5" velocity="0.5"/>
  </joint>

  <joint name="pantbl_to_cam" type="revolute">
    <parent link="pantbl_link"/>
    <child link="cam_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="-1.5" upper="1.5" velocity="0.5"/>
  </joint>

  <joint name="camera_joint" type="fixed">
    <axis xyz="0 1 0" />
    <origin xyz=".15 0 .33" rpy="0 0 0"/>
    <limit effort="1000.0" lower="-1.5" upper="1.5" velocity="0.5"/>
    <parent link="base_link"/>
    <child link="base_cam"/>
  </joint>

